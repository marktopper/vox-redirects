{
  "name": "vox-redirects",
  "description": "A Hook that allows you to add redirects to your Laravel and Voyager App.",
  "require": {
    "yogurt-design/laravel-hooks": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "VoxRedirects\\": "src/"
    }
  },
  "extra": {
    "hook": {
      "providers": [
        "VoxRedirects\\VoxRedirectServiceProvider"
      ]
    }
  }
}
